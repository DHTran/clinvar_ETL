<clinicalassertion datecreated="2020-06-19" datelastupdated="2020-06-22" id="2656043" submissiondate="2020-05-19">
<clinvarsubmissionid localkey="Db4KrQBSZeoE7-orUyypkc4jdGwFj472knnGDdT0Yh4|MedGen:C0027672" submittedassembly="GRCh37"></clinvarsubmissionid>
