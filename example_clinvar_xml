<name>
<elementvalue type="Alternate">FAMILIAL ADENOMATOUS POLYPOSIS 1, ATTENUATED</elementvalue>
<xref db="OMIM" id="611731.0034" type="Allelic variant"></xref>
<xref db="OMIM" id="611731.0036" type="Allelic variant"></xref>
